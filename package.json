{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "babel-plugin-prismjs": "^2.0.1",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-preset-gatsby": "^0.2.28",
    "gatsby": "^2.19.12",
    "gatsby-image": "^2.2.21",
    "gatsby-plugin-manifest": "^2.2.18",
    "gatsby-plugin-offline": "^3.0.8",
    "gatsby-plugin-prefetch-google-fonts": "^1.4.3",
    "gatsby-plugin-react-helmet": "^3.1.8",
    "gatsby-plugin-react-svg": "^2.1.2",
    "gatsby-plugin-sharp": "^2.2.25",
    "gatsby-plugin-sitemap": "^2.2.20",
    "gatsby-plugin-styled-components": "^3.1.6",
    "gatsby-source-filesystem": "^2.1.26",
    "gatsby-source-ghost": "^4.0.2",
    "gatsby-transformer-sharp": "^2.2.17",
    "jimp": "^0.8.5",
    "prismjs": "^1.19.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-flexbox-grid": "^2.1.2",
    "react-ga": "^2.7.0",
    "react-helmet": "^5.2.1",
    "react-share": "^3.0.1",
    "react-social-icons": "^4.1.0",
    "smooth-scroll": "^16.1.0",
    "styled-components": "^4.4.0"
  },
  "devDependencies": {
    "gh-pages": "^2.1.1",
    "husky": "^3.0.9",
    "prettier": "^1.18.2",
    "pretty-quick": "^2.0.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \"",
    "deploy": "gatsby build && gh-pages -d public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
